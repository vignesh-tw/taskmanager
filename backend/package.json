{
  "name": "task-manager-backend",
  "version": "1.0.0",
  "description": "Task Manager Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cross-env NODE_ENV=test mocha \"test/**/*.test.js\" --timeout 10000 --exit",
    "test:watch": "cross-env NODE_ENV=test mocha \"test/**/*.test.js\" --timeout 10000 --watch",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha \"test/**/*.test.js\" --timeout 10000 --exit",
    "lint": "echo 'No linting configured'"
  },
  "keywords": [
    "task",
    "manager",
    "backend",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "chai-as-promised": "^8.0.2",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^8.16.1",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "sinon": "^21.0.0",
    "supertest": "^6.3.4"
  },
  "nyc": {
    "exclude": [
      "test/**/*.js"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
